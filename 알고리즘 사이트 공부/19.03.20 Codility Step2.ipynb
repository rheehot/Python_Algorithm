    "def solution(A):\n",
    "    n = len(A)\n",
    "    A.sort()\n",
    "    for i in range(0,n-1,2):\n",
    "        if A[i] != A[i+1]:\n",
    "            return A[i]\n",
    "    return A[-1]\n",
    "\n",
    "A = [9,3,3,19,9,7,9,7,9]\n",
    "solution(A)"
   ]
